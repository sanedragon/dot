KeepAlive				yes
ServerAliveInterval		60
ForwardX11				yes
ForwardAgent			yes

Host voddev1 voddev2
	User estrickland

# Rentrak
Host voddev1.remote
	TcpKeepAlive	yes
	User			estrickland
	Hostname		voddev1.pdx.rentrak.com
	ForwardX11		yes
	ForwardAgent	yes
	IdentitiesOnly	yes
	IdentityFile	~/.ssh/id_rsa
	ProxyCommand	ssh rlogin1.remote nc %h %p
	DynamicForward	9000

