# EC2 tunnel
Host hypodermia.me
	Hostname		hypodermia.me
	User			hypodermia
	TcpKeepAlive	yes
	ForwardX11		yes
	ForwardAgent	yes
	IdentitiesOnly	yes
	IdentityFile	~/.ssh/id_rsa
	DynamicForward	8000

Host ec2
	Hostname		hypodermia.me
	User			ec2-user
	TcpKeepAlive	yes
	ForwardX11		yes
	ForwardAgent	yes
	IdentitiesOnly	yes
	IdentityFile	~/.ssh/hypodermia.pem
	DynamicForward	8000

# Rentrak
Host voddev1.remote
	TcpKeepAlive	yes
	User			estrickland
	Hostname		voddev1.pdx.rentrak.com
	ForwardX11		yes
	ForwardAgent	yes
	IdentitiesOnly	yes
	IdentityFile	~/.ssh/id_rsa
	ProxyCommand	ssh rlogin1.remote nc %h %p
	DynamicForward	9000

Host vodliveprocmaster1.remote
	TcpKeepAlive	yes
	User			estrickland
	Hostname		vodliveprocmaster1.vw.rentrak.com
	ForwardX11		yes
	ForwardAgent	yes
	IdentitiesOnly	yes
	IdentityFile	~/.ssh/id_rsa
	ProxyCommand	ssh rlogin2.remote nc %h %p
	DynamicForward	9000

Host rlogin1.remote
	TcpKeepAlive	yes
	User			estrickland
	Hostname		rlogin1.rentrak.com
	ForwardX11		yes
	ForwardAgent	yes
	IdentitiesOnly	yes
	IdentityFile	~/.ssh/id_rsa

Host rlogin2.remote
	TcpKeepAlive	yes
	User			estrickland
	Hostname		rlogin2.rentrak.com
	ForwardX11		yes
	ForwardAgent	yes
	IdentitiesOnly	yes
	IdentityFile	~/.ssh/id_rsa


